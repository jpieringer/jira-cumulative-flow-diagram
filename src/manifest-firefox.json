{
  "name": "Jira Cumulative Flow Diagram",
  "description": "This extension creates Cumulative Flow Diagrams based on Jira Queries.",
  "version": "14.0",
  "manifest_version": 2,
  "author": "Johannes Pieringer",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png",
    "512": "icon512.png"
  },
  "background": {
    "scripts": [
      "background-firefox.js"
    ]
  },
  "browser_action": {
    "default_title": "Jira Cumulative Flow Diagram",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "64": "icon64.png",
      "128": "icon128.png",
      "512": "icon512.png"
    }
  },
  "permissions": [
    "https://*/*",
    "<all_urls>"
  ]
}